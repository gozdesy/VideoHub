{
  "VideoHubWebSettings": {
    "ApiUrl": "https://localhost:7255"
  },
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "UpLogger": {
    "ConnectionString": "",
    "DatabaseName": "UpLoggerDB",
